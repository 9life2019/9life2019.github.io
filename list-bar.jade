    body(ontouchstart='')
        main.life-wrapper
            section.life-list
                .list-header
                    .list-header__tab
                        .list-header__tab__group
                            .list-header__tab__item.active(data-id='box-1')
                                | Listing
                            .list-header__tab__item(data-id='box-2')
                                | Map
                    #box-1.list-header__nav.list-header__box
