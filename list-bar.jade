                            .list-nav__item.active
                                a
                                    | Tất cả
                            .list-nav__item
                                a
                                    | Karaoke
                            .list-nav__item
                                a
                                    | Bar
                            .list-nav__item
                                a
                                    | Pub
                            .list-nav__item
                                a
                                    | Lounge
                        .filter-icon
                            i.fas.fa-sliders-h

                    #box-2.list-header__map.list-header__box.d-none
