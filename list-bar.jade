        script.
            $(".list-header__tab__item").click(function () {
                var tab_id = $(this).attr('data-id');

                $('.list-header__tab__item').removeClass('active');
                $('.list-header__box').addClass('d-none');

                $(this).addClass('active');
                $("#" + tab_id).removeClass('d-none');

            });



