    body(ontouchstart='')
        main.life-wrapper
            section.life-list
                .list-header
                    .list-header__top
                        .list-header__search
                            img(src='images/i-search.svg')
                        .list-header__tab
                            .list-header__tab__group
                                .list-header__tab__item.active(data-id='box-1')
                                    | Listing
                                .list-header__tab__item(data-id='box-2')
                                    | Map
                    .list-header__bottom
                        #box-1.list-header__nav.list-header__box
                            .list-nav
                                .list-nav__item.active
                                    a
                                        | Tất cả
                                .list-nav__item
                                    a
                                        | Karaoke
                                .list-nav__item
                                    a
                                        | Bar
                                .list-nav__item
                                    a
                                        | Pub
                                .list-nav__item
                                    a
                                        | Lounge
                            .filter-icon
                                img(src="images/i-sliders.svg")

                        #box-2.list-header__map.list-header__box.d-none
                            .form-group
                                select.form-control
                                    option Hà Nội
                                    option Tp. Hồ Chí Minh
                                    option Đà Nẵng
                            .form-group.active
                                select.form-control
                                    option Gần bạn
                                    option Xa bạn
                            .form-group
                                a.btn.btn-purple
                                    img(src='images/i-clock-white.svg')
                                    span Open Now
                .life-list__slide
                    p.life-list__title sponsored
                    .list-bar.owl-carousel.owl-theme
